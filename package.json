{
  "name": "dock-direct-connect",
  "version": "1.0.0",
  "description": "Direct freight connections between shippers and drivers",
  "main": "server/src/app.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "start": "cd server && npm start",
    "test": "npm run test:server && npm run test:client",
    "test:server": "cd server && npm test",
    "test:client": "cd client && npm test",
    "lint": "npm run lint:server && npm run lint:client",
    "lint:server": "cd server && npm run lint",
    "lint:client": "cd client && npm run lint",
    "format": "prettier --write .",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "setup": "npm run install:all && npm run db:generate && npm run db:push && npm run db:seed"
  },
  "keywords": [
    "freight",
    "logistics",
    "shipping",
    "trucking",
    "transportation",
    "saas",
    "react",
    "typescript",
    "nodejs"
  ],
  "author": "Dock Direct Connect Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "concurrently": "^8.2.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/dock-direct-connect.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/dock-direct-connect/issues"
  },
  "homepage": "https://github.com/your-org/dock-direct-connect#readme"
}
